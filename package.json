{
  "name": "@aboutbits/react-ui",
  "version": "1.2.2",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/aboutbits/react-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aboutbits/react-ui.git"
  },
  "homepage": "https://storybook.aboutbits.it",
  "engines": {
    "npm": "^8",
    "node": "^16"
  },
  "scripts": {
    "dev": "start-storybook -p 4000",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project tsconfig.build.esm.json",
    "build:cjs": "tsc --project tsconfig.build.cjs.json",
    "lint": "eslint --ext .js,.ts,.tsx,.jsx,.stories.mdx,.docs.mdx,.json .",
    "lint:fix": "eslint --ext .js,.ts,.tsx,.jsx,.stories.mdx,.docs.mdx,.json --fix .",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "build-storybook": "build-storybook",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "version": "npm run test && npm run lint && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@aboutbits/pagination": "^1.1.0",
    "@aboutbits/react-material-icons": "^1.1.2",
    "@aboutbits/react-pagination": "^0.0.10 || ^1.0.0",
    "@aboutbits/react-toolbox": "^0.1.2",
    "@headlessui/react": "^1.7.7",
    "axios": "^0.27.0 || ^1.1.0",
    "classnames": "^2.0.0",
    "react-hook-form": "^7.36.1"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@hookform/resolvers": "^2.9.10",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.5.15",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.2",
    "cssnano": "^5.1.14",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "next": "^13.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "yup": "^0.32.11"
  },
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "tailwind-preset.js"
  ]
}
