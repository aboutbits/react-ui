{
  "name": "@aboutbits/react-ui",
  "version": "1.5.1",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**/*",
    "tailwind-preset.js"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/aboutbits/react-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aboutbits/react-ui.git"
  },
  "homepage": "https://storybook.aboutbits.it",
  "engines": {
    "npm": "^8 || ^9",
    "node": "^16 || ^18"
  },
  "scripts": {
    "dev": "storybook dev -p 4000 --no-open",
    "build": "rimraf dist && npm run build:types && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "lint": "eslint --ext .js,.ts,.tsx,.jsx,.stories.mdx,.docs.mdx,.json --cache .",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch --passWithNoTests",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "build-storybook": "storybook build",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "version": "npm run test && npm run lint && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@aboutbits/pagination": "^1.1.0",
    "@aboutbits/react-material-icons": "^1.1.2",
    "@aboutbits/react-pagination": "^0.0.10 || ^1.0.0",
    "@aboutbits/react-toolbox": "^0.1.2",
    "@headlessui/react": "^1.7.7",
    "axios": "^1.4.0",
    "classnames": "^2.0.0",
    "react-hook-form": "^7.44.3"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/addon-mdx-gfm": "^7.0.26",
    "@storybook/addon-styling": "^1.3.2",
    "@storybook/react": "^7.0.26",
    "@storybook/react-vite": "^7.0.26",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.18",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "jsdom": "^22.1.0",
    "next": "^13.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-textarea-autosize": "^8.5.2",
    "rimraf": "^5.0.1",
    "storybook": "^7.0.26",
    "tailwindcss": "^3.2.4",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vitest": "^0.33.0",
    "yup": "^0.32.11"
  }
}
